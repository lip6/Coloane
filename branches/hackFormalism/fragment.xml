      <Formalism
            extension="model"
            fkname="Hybrid Automata"
            id="hyridautomata"
            image="resources/hclock_16.png"
            name="Hybrid Automata">
         <XmlDescription>
            <Graph
                  name="Hybrid Automata">
               <Node
                     container="false"
                     name="State">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.EllipseNode"
                        description="A state of an hybrid automaton"
                        drawable="true"
                        height="18"
                        icon16px="resources/state_16.png"
                        icon24px="resources/state_24.png"
                        palettable="true"
                        paletteName="State"
                        width="18">
                  </GraphicInfo>
                  <Attribute
                        drawable="true"
                        multiline="true"
                        name="Clock speed">
                  </Attribute>
                  <Attribute
                        drawable="true"
                        multiline="true"
                        name="Invariant">
                  </Attribute>
               </Node>
               <Arc
                     name="Transition">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.arcs.DirectedArc"
                        description="A transition of an hybrid automaton"
                        drawable="true"
                        icon16px="resources/arc_16.png"
                        icon24px="resources/arc_24.png"
                        palettable="true"
                        paletteName="Transition">
                  </GraphicInfo>
                  <Attribute
                        default="True"
                        drawable="true"
                        multiline="false"
                        name="Guard">
                  </Attribute>
                  <Attribute
                        drawable="true"
                        multiline="true"
                        name="Label">
                  </Attribute>
                  <Attribute
                        drawable="true"
                        multiline="true"
                        name="Reset">
                  </Attribute>
               </Arc>
               <Attribute
                     drawable="true"
                     multiline="false"
                     name="Analogic clocks">
               </Attribute>
               <Attribute
                     drawable="true"
                     multiline="false"
                     name="Digital clocks">
               </Attribute>
               <Node
                     container="false"
                     name="Initial state">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.EllipseNode"
                        description="An initial state of an hybrid automaton"
                        drawable="true"
