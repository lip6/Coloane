<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="fr.lip6.move.coloane.core.exports">
      <Export
            class="fr.lip6.move.coloane.extension.importExportRomeo.exportToRomeo.ExportToImpl"
            extension="cami"
            id="fr.lip6.move.coloane.extensions.exportromeo"
            reference="fr.lip6.move.coloane.wizard.export.romeo">
         <AcceptedFormalism
               id="Timed Petri Net">
         </AcceptedFormalism>
      </Export>
   </extension>
   <extension
         point="fr.lip6.move.coloane.core.imports">
      <Import
            class="fr.lip6.move.coloane.extension.importExportRomeo.importFromRomeo.ImportFromImpl"
            id="fr.lip6.move.coloane.extensions.importcami"
            reference="fr.lip6.move.coloane.wizard.import.cami">
         <AcceptedFormalism
               id="PT-Net">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="CPN">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="RG">
         </AcceptedFormalism>
      </Import>
   </extension>
   <extension
   		 point="org.eclipse.ui.exportWizards">
      <wizard
            category="fr.lip6.move.coloane.wizard.export.coloane"
            class="fr.lip6.move.coloane.core.ui.wizards.exportmodel.ExportWizard"
            icon="resources/camifile.png"
            id="fr.lip6.move.coloane.wizard.export.cami"
            name="Cami File">
		 <description>
            Export local models to your file system using CAMI syntax.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="fr.lip6.move.coloane.wizard.import.coloane"
            class="fr.lip6.move.coloane.core.ui.wizards.importmodel.ImportWizard"
            icon="resources/camifile.png"
            id="fr.lip6.move.coloane.wizard.import.cami"
            name="Cami File">
         <description>
            Import models written in CAMI from the local file system.
         </description>
      </wizard>
   </extension>

</plugin>
