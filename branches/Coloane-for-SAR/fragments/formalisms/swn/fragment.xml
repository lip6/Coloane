<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="fr.lip6.move.coloane.core.formalisms">
      <Formalism
            extension="swn"
            fkname="fr.lip6.move.coloane.formalism.cosmos.Formalism1"
            id="fr.lip6.move.coloane.formalism.swn"
            image="resources/red-dice-icon(petit).png"
            name="swn">
         <XmlDescription>
            <Graph
                  name="swn">
               <Node
                     container="true"
                     name="place">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.EllipseNode"
                        description="place"
                        drawable="true"
                        height="15"
                        icon16px="resources/place_16.png"
                        icon24px="resources/place_24.png"
                        palettable="true"
                        paletteName="Place"
                        width="15">
                  </GraphicInfo>
                  <Attribute
                        drawable="true"
                        multiline="true"
                        name="name">
                     <AttributeChecker
                           class_condition="fr.lip6.move.coloane.core.formalisms.checkers.EmptyAttributeChecker"
                           message="A place have no name"
                           name="Place Empty name"
                           severity="Error">
                     </AttributeChecker>
                  </Attribute>
                  <Attribute
                        bold="true"
                        displayed_default="true"
                        drawable="true"
                        multiline="true"
                        name="marking">
                  </Attribute>
                  <Attribute
                        delta_x="20"
                        delta_y="10"
                        drawable="true"
                        multiline="true"
                        name="domain">
                  </Attribute>
               </Node>
               <Node
                     container="true"
                     name="transition">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.RectangleNode"
                        description="transition"
                        drawable="true"
                        height="7"
                        icon16px="resources/transition_16.png"
                        icon24px="resources/transition_24.png"
                        palettable="true"
                        paletteName="Transition"
                        width="25">
                  </GraphicInfo>
                  <Attribute
                        drawable="true"
                        multiline="true"
                        name="name">
                     <AttributeChecker
                           class_condition="fr.lip6.move.coloane.core.formalisms.checkers.EmptyAttributeChecker"
                           message="There is a transition with no name"
                           name="Transition Empty name"
                           severity="Error">
                     </AttributeChecker>
                  </Attribute>
                  <Attribute
                        default="EXPONENTIAL"
                        drawable="true"
                        enumerated="false"
                        multiline="true"
                        name="distribution">
                  </Attribute>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="weight">
                  </Attribute>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="priority">
                  </Attribute>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="service">
                  </Attribute>
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.RectangleNode"
                        description="transition"
                        drawable="true"
                        height="25"
                        icon16px="resources/transition_16.png"
                        icon24px="resources/transition_24.png"
                        palettable="true"
                        paletteName="Transition"
                        width="7">
                  </GraphicInfo>
                  <Attribute
                        delta_x="20"
                        delta_y="10"
                        drawable="true"
                        multiline="true"
                        name="guard">
                  </Attribute>
               </Node>
               <Arc
                     name="arc">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.arcs.DirectedArc"
                        description="arc"
                        drawable="true"
                        icon16px="resources/arc_16.png"
                        icon24px="resources/arc_24.png"
                        palettable="true"
                        paletteName="arc">
                  </GraphicInfo>
                  <Attribute
                        bold="true"
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="false"
                        name="valuation">
                  </Attribute>
               </Arc>
               <Arc
                     name="inhibitorarc">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.arcs.InhibitorArc"
                        description="InhibitorArc"
                        drawable="true"
                        icon16px="resources/inhibitor_16.png"
                        icon24px="resources/inhibitor_24.png"
                        palettable="true"
                        paletteName="InhibitorArc">
                  </GraphicInfo>
                  <Attribute
                        bold="true"
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="false"
                        name="valuation">
                  </Attribute>
               </Arc>
               <Attribute
                     drawable="true"
                     multiline="true"
                     name="declarations">
               </Attribute>
            </Graph>
            <Constraint
                  link="true"
                  name="place-place"
                  type="fr.lip6.move.coloane.interfaces.formalism.constraints.ConnectionConstraint">
               <parameter
                     name="source"
                     value="place">
               </parameter>
               <parameter
                     name="target"
                     value="place">
               </parameter>
            </Constraint>
            <Constraint
                  link="true"
                  name="transition-transition"
                  type="fr.lip6.move.coloane.interfaces.formalism.constraints.ConnectionConstraint">
               <parameter
                     name="source"
                     value="transition">
               </parameter>
               <parameter
                     name="target"
                     value="transition">
               </parameter>
            </Constraint>
            <Constraint
                  link="true"
                  name="Inhibitor Arc"
                  type="fr.lip6.move.coloane.interfaces.formalism.constraints.ConnectionConstraint">
               <parameter
                     name="source"
                     value="transition">
               </parameter>
               <parameter
                     name="target"
                     value="place">
               </parameter>
               <parameter
                     name="arcType"
                     value="inhibitorarc">
               </parameter>
            </Constraint>
         </XmlDescription>
      </Formalism>
   </extension>

</fragment>
