            description="Layout a Coloane model using GraphViz"
            id="fr.lip6.move.coloane.graphviz.layout"
            name="layout">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="fr.lip6.move.coloane.ui.dialogs.ColoanePreference"
