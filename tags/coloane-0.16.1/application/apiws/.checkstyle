<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <local-check-config name="Coloane Checkstyle" location="http://coloane.lip6.fr/resources/coloane_checkstyle.xml" type="remote" description="Checkstyle for Coloane's projects">
        <additional-data name="cache-file" value="false"/>
    </local-check-config>
    <fileset name="tous" enabled="true" check-config-name="Coloane Checkstyle" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
        <file-match-pattern match-pattern="WrapperStub.java" include-pattern="false"/>
        <file-match-pattern match-pattern="WrapperCallbackHandler.java" include-pattern="false"/>
        <file-match-pattern match-pattern="GExceptionException0.java" include-pattern="false"/>
    </fileset>
</fileset-config>
