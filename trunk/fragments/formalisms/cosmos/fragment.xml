<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="fr.lip6.move.coloane.core.formalisms">
      <Formalism
            extension="cosmos"
            fkname="fr.lip6.move.coloane.formalism.cosmos.Formalism1"
            id="fr.lip6.move.coloane.formalism.cosmos.gspn"
            name="Cosmos">
         <XmlDescription>
            <Graph
                  name="Cosmos">
               <Node
                     container="true"
                     name="place">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.EllipseNode"
                        description="place"
                        drawable="true"
                        height="15"
                        icon16px="resources/place_16.png"
                        icon24px="resources/place_24.png"
                        palettable="true"
                        paletteName="Place"
                        width="15">
                  </GraphicInfo>
                  <Attribute
                        drawable="true"
                        multiline="true"
                        name="name">
                  </Attribute>
                  <Attribute
                        default="0"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="marking">
                  </Attribute>
               </Node>
               <Node
                     container="true"
                     name="transition">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.RectangleNode"
                        description="transition"
                        drawable="true"
                        height="7"
                        icon16px="resources/transition_16.png"
                        icon24px="resources/transition_24.png"
                        palettable="true"
                        paletteName="Transition"
                        width="25">
                  </GraphicInfo>
                  <Attribute
                        default="Exponential"
                        drawable="true"
                        enumerated="false"
                        multiline="true"
                        name="Distribution">
                  </Attribute>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="Weight">
                  </Attribute>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="Priority">
                  </Attribute>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="Service">
                  </Attribute>
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.nodes.RectangleNode"
                        description="transition"
                        drawable="true"
                        height="25"
                        icon16px="resources/transition_16.png"
                        icon24px="resources/transition_24.png"
                        palettable="true"
                        paletteName="Transition"
                        width="7">
                  </GraphicInfo>
               </Node>
               <Arc
                     name="arc">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.arcs.DirectedArc"
                        description="arc"
                        drawable="true"
                        icon16px="resources/arc_16.png"
                        icon24px="resources/arc_24.png"
                        palettable="true"
                        paletteName="arc">
                  </GraphicInfo>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="valuation">
                  </Attribute>
               </Arc>
               <Arc
                     name="inhibitorarc">
                  <GraphicInfo
                        associatedFigure="fr.lip6.move.coloane.core.ui.figures.arcs.InhibitorArc"
                        description="InhibitorArc"
                        drawable="true"
                        icon16px="resources/inhibitor_16.png"
                        icon24px="resources/inhibitor_24.png"
                        palettable="true"
                        paletteName="InhibitorArc">
                  </GraphicInfo>
                  <Attribute
                        default="1"
                        displayed_default="false"
                        drawable="true"
                        multiline="true"
                        name="valuation">
                  </Attribute>
               </Arc>
               <Attribute
                     drawable="true"
                     multiline="true"
                     name="const definition">
               </Attribute>
            </Graph>
         </XmlDescription>
      </Formalism>
   </extension>

</fragment>
