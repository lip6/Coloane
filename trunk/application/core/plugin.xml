    <report
          class="fr.lip6.move.coloane.core.results.reports.IsTheNetQuestionReport"
          service_name="Is the net structuraly safe?">
    </report>
    <report
          class="fr.lip6.move.coloane.core.results.reports.MinimalSyphonTrapReport"
          service_name="Minimal syphon">
    </report>
    <report
          class="fr.lip6.move.coloane.core.results.reports.MinimalSyphonTrapReport"
          service_name="Minimal traps">
    </report>
    <report
          class="fr.lip6.move.coloane.core.results.reports.ComputeBoundsReport"
          service_name="Compute structural bounds by unfolding">
    </report>
    <report
          class="fr.lip6.move.coloane.core.results.reports.IsTheNetQuestionReport"
          service_name="Is the colored net structurally bounded?">
    </report>
    <report
          class="fr.lip6.move.coloane.core.results.reports.IsTheNetQuestionReport"
          service_name="Is the colored structurally net safe (by unfolding)?">
    </report>
 </extension>
 <extension
       point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
    <propertyContributor
          contributorId="fr.lip6.move.coloane.properties.contributor"
