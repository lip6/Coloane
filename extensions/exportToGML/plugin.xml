<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="exportGML" name="exportGML" schema="schema/exportGML.exsd"/>
   <extension
         point="fr.lip6.move.coloane.core.exports">
      <Export
            class="fr.lip6.move.coloane.extensions.exporttogml.ExportToGML"
            extension="gml"
            id="fr.lip6.move.coloane.extensions.exporttogml"
            wizard_id="fr.lip6.move.coloane.wizard.export.gml">
         <AcceptedFormalism
               id="PT-Net">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="CPN">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="SNB">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="fr.lip6.move.coloane.formalism.cosmos.gspn">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="Timed Automata">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="fr.lip6.move.coloane.formalism.cosmosLHA">
         </AcceptedFormalism>
         <AcceptedFormalism
               id="fr.lip6.move.coloane.formalism.pta">
         </AcceptedFormalism>
      </Export>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <wizard
            category="fr.lip6.move.coloane.wizard.export.coloane"
            class="fr.lip6.move.coloane.core.ui.wizards.exportmodel.ExportWizard"
            id="fr.lip6.move.coloane.wizard.export.gml"
            name="GML file">
      </wizard>
   </extension>
   <extension
         point="fr.lip6.move.coloane.extensions.exportToGML.exportGML">
      <exportedFormalism
            class="fr.lip6.move.coloane.extensions.exporttogml.SNBExport"
            fmlurl="http://formalisms.cosyverif.org/s-net.fml"
            name="CPN">
      </exportedFormalism>
      <exportedFormalism
            class="fr.lip6.move.coloane.extensions.exporttogml.PTExport"
            fmlurl="http://formalisms.cosyverif.org/pt-net.fml"
            name="PT-Net">
      </exportedFormalism>
      <exportedFormalism
            class="fr.lip6.move.coloane.extensions.exporttogml.SNBExport"
            fmlurl="http://formalisms.cosyverif.org/snb.fml"
            name="SNB">
      </exportedFormalism>
      <exportedFormalism
            class="fr.lip6.move.coloane.extensions.exporttogml.CosmosExport"
            fmlurl="http://formalisms.cosyverif.org/sptgd-net.fml"
            name="fr.lip6.move.coloane.formalism.cosmos.gspn">
      </exportedFormalism>
      <exportedFormalism
            class="fr.lip6.move.coloane.extensions.exporttogml.CosmosExport"
            fmlurl="http://formalisms.cosyverif.org/HASL-formula.fml"
            name="fr.lip6.move.coloane.formalism.cosmosLHA">
      </exportedFormalism>
      <exportedFormalism
            class="fr.lip6.move.coloane.extensions.exporttogml.ImitatorExport"
            description="Parametric timed automata"
            fmlurl="http://formalisms.cosyverif.org/parametric-timed-automaton.fml"
            name="fr.lip6.move.coloane.formalism.pta">
      </exportedFormalism>
   </extension>

</plugin>
