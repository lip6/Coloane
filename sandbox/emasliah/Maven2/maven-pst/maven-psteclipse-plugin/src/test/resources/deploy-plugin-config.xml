<?xml version="1.0" encoding="UTF-8"?>

<project>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-psteclipse-plugin</artifactId>
				<configuration>
					<eclipseHome>src/test/resources/eclipse</eclipseHome>
					<repositoryHome>target</repositoryHome>
					<repositoryId>pstapollodev</repositoryId>
					<url>scp://nobox/norepo</url>
				</configuration>
				<executions>
					<execution>
						<id>deploy</id>
						<goals>
							<goal>deploy</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
