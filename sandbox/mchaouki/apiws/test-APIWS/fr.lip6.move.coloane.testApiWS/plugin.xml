<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="fr.lip6.move.coloane.testApiWS.actionSet">
         <action
               class="fr.lip6.move.coloane.testapiws.actions.OpenConnectionAction"
               id="fr.lip6.move.coloane.testapiws.actions.OpenConnectionAction"
               label="&amp;Ouvrir une connexion"
               menubarPath="sampleMenu/sampleGroupApiWS"
               tooltip="Ouvrir une connexion">
         </action>
         <action
               class="fr.lip6.move.coloane.testapiws.actions.OpenSessionAction"
               id="fr.lip6.move.coloane.testApiWS.action1"
               label="&amp;Ouvrir une session"
               menubarPath="sampleMenu/sampleGroupApiWS"
               tooltip="Ouvrir une session">
         </action>
         <action
               class="fr.lip6.move.coloane.testapiws.actions.CloseSessionAction"
               id="fr.lip6.move.coloane.testApiWS.action2"
               label="Fermer session"
               menubarPath="sampleMenu/sampleGroupApiWS"
               tooltip="Fermer la session">
         </action>
         <action
               class="fr.lip6.move.coloane.testapiws.actions.CloseConnectionAction"
               id="fr.lip6.move.coloane.testApiWS.action3"
               label="Fermer Connexion"
               menubarPath="sampleMenu/sampleGroupApiWS"
               tooltip="Fermer Connexion">
         </action>
         <menu
               id="sampleMenu"
               label="ApiWS">
         </menu>
      </actionSet>
   </extension>

</plugin>
